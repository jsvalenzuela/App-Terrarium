<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_principal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="net.londatiga.android.bluetooth.AutomaticoTransferenciaArduino"
    android:theme="@style/AppTheme">

    <TextView
        android:id="@+id/tv_Temperatura"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="74dp"
        android:text="Temperatura"
        android:textAppearance="@style/TextAppearance.AppCompat.Small" />

    <TextView
        android:id="@+id/tv_SensorTemp"
        android:layout_width="121dp"
        android:layout_height="45dp"
        android:layout_alignTop="@+id/tv_Temperatura"
        android:layout_alignBottom="@+id/tv_Temperatura"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="62dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="1dp"
        android:layout_toEndOf="@+id/tv_Temperatura"
        android:layout_toRightOf="@+id/tv_Temperatura"
        android:textAppearance="@android:style/TextAppearance.Material.Medium" />

    <TextView
        android:id="@+id/tv_Humedad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_Temperatura"
        android:layout_marginTop="17dp"
        android:text="Humedad"
        android:textAppearance="@style/TextAppearance.AppCompat.Small" />

    <TextView
        android:id="@+id/tv_SensorHumedad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_SensorTemp"
        android:layout_alignStart="@+id/tv_SensorTemp"
        android:layout_alignLeft="@+id/tv_SensorTemp"
        android:layout_alignBottom="@+id/tv_Humedad"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="2dp"
        android:textAppearance="@android:style/TextAppearance.Material.Medium" />

    <TextView
        android:id="@+id/tvEstadoRiego"
        android:layout_width="111dp"
        android:layout_height="49dp"
        android:layout_below="@+id/textView"
        android:layout_marginTop="280dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="150dp"
        android:layout_height="34dp"
        android:layout_below="@+id/tv_Humedad"
        android:layout_marginTop="17dp"
        android:text="Humedad Tipo Planta" />

    <TextView
        android:id="@+id/tvHumedadServicio"
        android:layout_width="89dp"
        android:layout_height="44dp"
        android:layout_below="@+id/tv_SensorHumedad"
        android:layout_marginStart="122dp"
        android:layout_marginLeft="122dp"
        android:layout_marginTop="18dp"
        android:layout_toEndOf="@+id/textView"
        android:layout_toRightOf="@+id/textView" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="95dp"
        android:layout_height="36dp"
        android:layout_below="@+id/textView"
        android:layout_marginTop="25dp"
        android:text="Tipo de planra" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="50dp"
        android:text="Suelo" />

    <TextView
        android:id="@+id/tvTipoPlantaTrans"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView2"
        android:layout_marginStart="168dp"
        android:layout_marginLeft="168dp"
        android:layout_marginTop="18dp"
        android:layout_toEndOf="@+id/textView2"
        android:layout_toRightOf="@+id/textView2"
        android:text="TextView" />

    <TextView
        android:id="@+id/tvSueloTrans"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView3"
        android:layout_marginStart="223dp"
        android:layout_marginLeft="223dp"
        android:layout_marginTop="-6dp"
        android:layout_toEndOf="@+id/textView3"
        android:layout_toRightOf="@+id/textView3"
        android:text="TextView" />


</RelativeLayout>